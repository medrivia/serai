{
    "name": "serai",
    "version": "260.0.0-alpha.3",
    "author": "Umar Alfarouk <medrivia@gmail.com> (https://alfarouk.id)",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/medrivia/serai.git"
    },
    "devDependencies": {
        "@swc/cli": "^0.7.7",
        "@swc/plugin-transform-imports": "^8.0.3",
        "es-toolkit": "^1.39.4",
        "redom": "^4.3.0",
        "remark-man": "^9.0.0",
        "remark-parse": "^11.0.0",
        "snarkdown": "^2.0.0",
        "unified": "^11.0.5"
    },
    "exports": {
        ".": "./src/v257/index.ts",
        "./*": "./src/*/index.ts"
    },
    "bin": {
        "serai": "bin/serai.ts"
    },
    "bugs": {
        "url": "https://github.com/medrivia/serai/issues"
    },
    "description": "ðŸ“– (WIP) Markdown-powered framework that focuses on freedom and interactivity.",
    "files": ["dist", "man"],
    "homepage": "https://github.com/medrivia/serai",
    "keywords": ["docs", "documentation", "framework", "knowledgebase", "markdown", "wiki"],
    "license": "GPL-3.0",
    "directories": {
        "man": "man"
    },
    "publishConfig": {
        "main": "./dist/v257/index.js",
        "types": "./dist/v257/index.d.ts",
        "exports": {
            ".": {
                "default": "./dist/v257/index.js",
                "types": "./dist/v257/index.d.ts"
            },
            "./*": {
                "default": "./dist/*/index.js",
                "types": "./dist/*/index.d.ts"
            }
        }
    },
    "scripts": {
        "b": "tsc --build tsconfig.build.json && swc --strip-leading-paths ./src/v*/ -d ./dist/",
        "prepublishOnly": "./man-generate.ts"
    },
    "type": "module"
}
